<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.provaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ModeCreate_2_1.CreateActivity"></activity>
        <activity
            android:name=".OperativeActivityChanger_1.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ModeJoin_2_0.JoinActivity"
            android:parentActivityName=".OperativeActivityChanger_1.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OperativeActivityChanger_1.MainActivity" />
        </activity>
    </application>

</manifest>